            turtle<span style="color: #333333">.</span>lt(<span style="color: #0000DD; font-weight: bold">90</span>)
            turtle<span style="color: #333333">.</span>fd(<span style="color: #0000DD; font-weight: bold">20</span>)
            turtle<span style="color: #333333">.</span>pd()
        <span style="color: #008800; font-weight: bold">elif</span> character <span style="color: #333333">==</span> <span style="color: #0000DD; font-weight: bold">98</span>: <span style="color: #888888">#displays created code for &#39;b&#39;</span>
            turtle<span style="color: #333333">.</span>left(<span style="color: #0000DD; font-weight: bold">90</span>)
